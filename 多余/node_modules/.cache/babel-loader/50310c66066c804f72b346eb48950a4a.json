rt default {\r\n    name: 'Count',\r\n    data() {\r\n        return {\r\n            n: 1, //用户选择数字\r\n            sum: 0, //和\r\n        }\r\n    },\r\n    methods: {\r\n        increment() {\r\n            this.sum += this.n\r\n        },\r\n        decrement() {\r\n            this.sum -= this.n\r\n        },\r\n        incrementOdd() {\r\n            if (this.sum % 2) {\r\n                this.sum += this.n\r\n            }\r\n        },\r\n        incrementWait() {\r\n            setTimeout(() => {\r\n                this.sum += this.n\r\n            }, 1000)\r\n        },\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\nbutton {\r\n    margin-left: 10px;\r\n}\r\n</style>"]}]}